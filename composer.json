{
    "name": "alikhedmati/laravel-kyc",
    "description": "Laravel KYC",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alikhedmati\\Kyc\\KycServiceProvider"
            ],
            "aliases": {
                "Kyc": "Alikhedmati\\Kyc\\Facades\\Kyc"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Alikhedmati\\Kyc\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ali Khedmati",
            "email": "ali@khedmati.ir"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
